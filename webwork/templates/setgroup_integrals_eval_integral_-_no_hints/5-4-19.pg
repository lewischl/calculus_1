DOCUMENT();

loadMacros(
  "PGstandard.pl",
  "PGchoicemacros.pl",
  "MathObjects.pl",
  "contextFraction.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,6,1);
$b = random(2,6,1);
$c = random(2,6,1);

TEXT(EV2(<<EOT));

Evaluate the definite integral:
$BR
$PAR
   \( \displaystyle{ \int_{-$a}^{$b} \left($c x - e^x\right) \,dx} = \)
   \{ans_rule(35) \}
EOT

$ans = $c*($b**2-$a**2)/2-(exp($b)-exp(-$a));
ANS(num_cmp($ans));

Context("Fraction");
$c2=Compute("$c/2");
$A=Compute("$c2*( $b)*($b)-$c2*$a*$a");
if ($c2==1) {$c2 =''};
if ($A==0) {$A =''};
Context()->texStrings;
BEGIN_SOLUTION
$PAR SOLUTION $PAR
\[ \begin{array}{rcl} \displaystyle \int_{-$a}^{$b} \left($c x - e^x\right) \,dx
& =& \displaystyle    $c2 x^2 - e^x\Big]_{-$a}^{$b} 
\\ & =&  \left( $c2  ($b)^2 - e^{$b}\right) - \left( $c2 (-$a)^2 - e^{-$a}\right) 
\\ &=& $A-e^{$b}+e^{-$a}
\end{array}
\]
END_SOLUTION

