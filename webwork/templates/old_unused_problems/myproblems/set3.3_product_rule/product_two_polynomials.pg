DOCUMENT();        # This should be the first executable line in the problem.

loadMacros(
  "PGstandard.pl",
  "MathObjects.pl",
  "PGcourse.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

Context('Numeric');

$an = random(2,5,1);
$as = random(-1,1,2);
$a  = $an * $as;
$n  = random(2,8,1);

$funct1 =  Compute("$a*x^($n-1)*($n+x)*e**x");

BEGIN_TEXT
Let \[ f(x) = $a x^ {$n} e^x \]
$BR $BR
\( f'( x ) = \) \{ans_rule(40) \}
END_TEXT

ANS($funct1->cmp);


ENDDOCUMENT();        # This should be the last executable line in the problem.
