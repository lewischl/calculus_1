TEXT(beginproblem());

Context("Numeric");

$a=random(3,9,1);
$b=random(1,9,1);

$b2 = -2*$b;
$b6 = 6 *$b;


$f = Formula("$a x^3 - $b*1/x");

$fd1 = $f->D;
$fd2 = $fd1->D;
$fd3 = $fd2->D;
