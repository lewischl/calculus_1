rand_button();


TEXT(beginproblem());

###################################
# Setup
Context()->variables->add(t=>'Real',dt=>'Real');
$a=random(1,9,1);

if($a==4 ){$a=10}
$A=$a+1;
$b=$A**2-$a**2;
$f=Formula("sqrt(t**2+$b)");
$F=$f->substitute(t=>'x');
$G=Formula("x/$F");
$fa=Compute("0");
$Fa=Compute("$A");
Context("Fraction");
$Ga=Compute("$a/$A");
Context("Numeric");







$mc = new_checkbox_multiple_choice();
$mc -> qa (
" Let \( \displaystyle G(x) = \int_4^{x^3} \sin t \, dt \).  $PAR Which of the following statements are correct?  Select correct options. $HR ", 
"\(G\) is the composite function \(A(x^3) \), where \( \displaystyle A(x) = \int_4^x \sin t \, dt \) $PAR",
"\(G'(x) = 3x^2 \sin(x^3) \) $PAR ",
"The Chain Rule is used to differentiate \(G\) $PAR",
"The Fundamental Theorem of Calculus Part 2 is used to differentiate \(G\) $PAR",
);
$mc -> extra(
"\(G\) is too complicated to differentiate $PAR",
"The product rule is used to differentiate \(G\) $PAR",
"\(G\) is the composite function \(\sin(x^3)\)$PAR",
);

    
    
    
    
